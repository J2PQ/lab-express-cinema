<div class="container">
  <div class="row">
    {{#each movies}}
    <div class="col-md-4 mb-3 ">
      <div class="card card-body">
        <img src={{image}} alt="">
        <h3>{{title}}</h3>
        <a class="btn btn-primary" href="/details/movD/{{_id}}" role="button">See More</a>
      </div>
    </div>
    {{/each}}
  </div>
</div>